# .env.example - Variables de entorno para Vaultwarden

# Token de administrador generado con Argon2
# Importante: reemplaza el primer $ por $$
ADMIN_TOKEN=$$argon2id$v=19$m=4096,t=3,p=1$example$hashvalue

# Dominio donde se alojará Vaultwarden (opcional para producción)
DOMAIN=https://ejemplo.com

# Habilitar WebSocket (recomendado si usas nginx o Apache con configuración adecuada)
WEBSOCKET_ENABLED=true

# Permitir registros de nuevos usuarios
SIGNUPS_ALLOWED=true
